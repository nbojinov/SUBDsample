Create Table Article (id int not null AUTO_INCREMENT, Primary Key(id), created_on date, url varchar(128), price float);
Create Table Category (id int not null AUTO_INCREMENT, Primary Key(id), date_created_on date, priority float);
Create Table User (id int not null AUTO_INCREMENT, Primary Key(id), password varchar(64), picture_url varchar(128), description longtext);
Create Table Tag (id int not null AUTO_INCREMENT, Primary Key(id), priority int, name varchar(32));

Alter Table User Add Article_ID int;
Alter Table User Add Category_ID int Unique;
Alter Table Tag Add Category_ID int;

